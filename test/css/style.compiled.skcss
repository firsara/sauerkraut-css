.no-js .jsinfo {
  höhe: 92px;
  breite: 460px;
  links: 50%;
  oben: 50%;
  position: absolut;
  hintergrund: url('../bild/nojs_info.png');
  außenrand: -46px 0 0 -230px;
}
.no-js artikel,
.no-js nav,
.no-js footer,
.no-js #headerimg,
.no-js #socialbar,
.no-js #downloads {
  anzeige: keines;
}
* {
  außenrand: 0;
  innenrand: 0;
}
artikel,
nav liste.ninesixty,
ul#socialbar,
#downloads #wrapper,
#downloads #wrapper ul,
#overview,
#converter,
#about,
footer,
.links,
.rechts {
  überlauf: versteckt;
}
körper {
  schrift-familie: "Helvetica Neue", Helvetica, Arial, sans-serif;
  schrift-größe: 13px;
  line-höhe: 18px;
  hintergrund: url('../bild/body_bg.jpg') repeat oben links;
}
#loader {
  anzeige: keines;
  position: fixed;
  breite: 100%;
  höhe: 100%;
  hintergrund: url('../bild/loader.png') no-repeat center center rgba(255, 255, 255, 0.5);
  ebene: 1000;
}
.de {
  anzeige: keines;
}
ü1 {
  schrift-gewicht: normal;
  farbe: #363636;
  tabellen-ausrichtung: center;
  schrift-größe: 22px;
  außenrand-unten: 50px;
}
ü1:davor,
h1:danach {
  anzeige: block;
  höhe: 2px;
  breite: 424px;
  außenrand: 7px automatisch 5px auto;
  inhalt: "";
  hintergrund: url('../bild/sprite.png') no-repeat 0 0;
}
ü2 {
  schrift-gewicht: normal;
  schrift-größe: 20px;
  farbe: #363636;
  außenrand: 10px 0;
}
liste {
  listen-stil-type: keines;
}
absatz {
  farbe: #424242;
}
mark {
  schrift-gewicht: fett;
  hintergrund: keines;
  farbe: #363636;
}
artikel {
  innenrand: 40px 0;
}
form {
  außenrand: 30px 0;
}
form absatz {
  tabellen-ausrichtung: center;
  schrift-größe: 14px;
  schrift-gewicht: fett;
  farbe: #717171;
  außenrand: 5px 0;
}
.links {
  breite: 480px;
  gleiten: links;
}
.rechts {
  gleiten: right;
}
.ninesixty {
  breite: 960px;
  außenrand: automatisch;
}
.CodeMirror {
  rahmen: 2px durchgezogen #dfdfdf;
  breite: 425px;
  regröße: vertical;
}
.knopf {
  anzeige: inline-block;
  kontur: keines;
  zeiger: pointer;
  tabellen-ausrichtung: center;
  text-dekoration: keines;
  schrift: 14px/100% Arial, Helvetica, sans-serif;
  innenrand: .5em 2em .55em;
  text-schatten: 0 1px 1px rgba(0, 0, 0, 0.3);
  -webkit-rahmen-radius: .3em;
  -moz-border-radius: .3em;
  border-radius: .3em;
  -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
  -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
}
.knopf:über {
  text-dekoration: keines;
}
.knopf + absatz {
  farbe: #363636;
  schrift-größe: 10px;
  tabellen-ausrichtung: center;
  innenrand-oben: 2px;
}
/* black */
.black {
  farbe: #d7d7d7;
  rahmen: durchgezogen 1px #333;
  hintergrund: #3b3b3b;
  background: -webkit-linear-gradient(oben, #5c6068, #3b3b3b);
  background: -moz-linear-gradient(top, #5c6068, #3b3b3b);
  background: -o-linear-gradient(top, #5c6068, #3b3b3b);
  background: -ms-linear-gradient(top, #5c6068, #3b3b3b);
  background: linear-gradient(top, #5c6068, #3b3b3b);
}
.black:über {
  hintergrund: #3b3b3b;
  background: -webkit-linear-gradient(oben, #54565b, #3b3b3b);
  background: -moz-linear-gradient(top, #54565b, #3b3b3b);
  background: -o-linear-gradient(top, #54565b, #3b3b3b);
  background: -ms-linear-gradient(top, #54565b, #3b3b3b);
  background: linear-gradient(top, #54565b, #3b3b3b);
}
/* grün */
.green {
  farbe: #d7d7d7;
  rahmen: durchgezogen 1px #899f57;
  hintergrund: #c2d057;
  background: -webkit-linear-gradient(oben, #c2d057, #8ca15b);
  background: -moz-linear-gradient(top, #c2d057, #8ca15b);
  background: -o-linear-gradient(top, #c2d057, #8ca15b);
  background: -ms-linear-gradient(top, #c2d057, #8ca15b);
  background: linear-gradient(top, #c2d057, #8ca15b);
}
.grün:über {
  hintergrund: #3b3b3b;
  background: -webkit-linear-gradient(oben, #c1cf56, #a1b76e);
  background: -moz-linear-gradient(top, #c1cf56, #a1b76e);
  background: -o-linear-gradient(top, #c1cf56, #a1b76e);
  background: -ms-linear-gradient(top, #c1cf56, #a1b76e);
  background: linear-gradient(top, #c1cf56, #a1b76e);
}
nav {
  höhe: 45px;
  hintergrund: url('../bild/nav_bg.jpg') repeat oben links;
  box-shadow: 0 3px 8px #888;
  position: fixed;
  breite: 100%;
  top: 0;
  ebene: 999;
}
nav liste.ninesixty listenelement {
  gleiten: rechts;
  schrift-größe: 13px;
  innenrand: 13px 0;
}
nav liste.ninesixty listenelement:erstes-kind {
  gleiten: links;
  schrift-größe: 15px;
  farbe: white;
}
nav liste.ninesixty listenelement:erstes-kind anker,
nav ul.ninesixty li:first-child a:über {
  farbe: white;
}
nav liste.ninesixty listenelement:nicht(:erstes-kind) anker {
  innenrand: 15px 20px;
}
nav liste.ninesixty listenelement:nicht(:erstes-kind) anker.aktiv {
  -webkit-box-shadow: inset 0 5px 5px 2px rgba(0, 0, 0, 0.5);
  -moz-box-shadow: inset 0 5px 5px 2px rgba(0, 0, 0, 0.5);
  box-shadow: inset 0 5px 5px 2px rgba(0, 0, 0, 0.5);
  farbe: white;
}
nav liste.ninesixty listenelement:nicht(:erstes-kind) anker:über {
  farbe: #fff;
}
nav liste.ninesixty listenelement anker {
  farbe: lightgrau;
  text-dekoration: keines;
}
nav .langflags {
  position: absolut;
  rechts: 15px;
  außenrand-oben: -11px;
  top: 50%;
}
nav .langflags anker {
  text-dekoration: keines;
}
nav .langflags anker bild {
  außenrand: 5px;
}
#headerimg {
  breite: 520px;
  höhe: 188px;
  außenrand: 100px automatisch 0 auto;
  innenrand-oben: 90px;
  hintergrund: url('../bild/sprite.png') no-repeat 0 -54px;
}
#headerimg #logo {
  hintergrund: url('../bild/sprite.png') no-repeat 0 -383px;
  höhe: 114px;
  breite: 372px;
  außenrand: automatisch;
}
liste#socialbar {
  breite: 508px;
  höhe: 41px;
  außenrand: 70px automatisch 0 auto;
  hintergrund: url('../bild/sprite.png') no-repeat 0 -547px;
}
liste#socialbar listenelement {
  farbe: #474a50;
  schrift-größe: 12px;
  gleiten: links;
  anzeige: block;
  innenrand: 10px 8px;
}
liste#socialbar listenelement:erstes-kind {
  innenrand-rechts: 10px;
}
liste#socialbar listenelement:ntes-kind(2) {
  breite: 110px;
}
liste#socialbar listenelement:ntes-kind(3) {
  breite: 70px;
}
liste#socialbar listenelement:letzes-kind {
  breite: 100px;
  außenrand-links: 10px;
}
#socialbar_dots {
  breite: 8px;
  höhe: 28px;
  außenrand: 10px automatisch;
  hintergrund: url('../bild/sprite.png') no-repeat 0 -638px;
}
#downloads {
  höhe: 103px;
  hintergrund: url('../bild/downloads_bg.png') repeat oben center;
}
#downloads #wrapper {
  breite: 780px;
  außenrand: automatisch;
  innenrand: 30px;
}
#downloads #wrapper #links,
#downloads #wrapper #rechts {
  breite: 360px;
  farbe: #363636;
  schrift-größe: 13px;
}
#downloads #wrapper #links {
  tabellen-ausrichtung: rechts;
  gleiten: links;
}
#downloads #wrapper #right {
  gleiten: rechts;
  außenrand-oben: -5px;
}
#downloads #wrapper liste listenelement {
  gleiten: links;
  außenrand: 5px;
}
#downloads #wrapper liste listenelement:ntes-kind(2) {
  innenrand: 5px 0;
}
#overview {
  innenrand: 40px 0 0 0;
  hintergrund: url('../bild/overview_bg.png') no-repeat center unten;
}
#usage liste {
  außenrand: 20px 0;
}
#usage liste listenelement {
  außenrand: 5px;
  farbe: #424242;
}
#usage liste listenelement:davor {
  inhalt: " - ";
}
#usage .CodeMirror-scroll {
  höhe: 25px !wichtig;
}
#usage #serverside .CodeMirror:letzes-kind .CodeMirror-scroll {
  höhe: 75px !wichtig;
}
#usage .spacer {
  höhe: 5px;
}
#converter {
  hintergrund: url('../bild/converter_bg.png') no-repeat center unten;
}
#about {
  hintergrund: url('../bild/about_bg.png') no-repeat center bottom;
}
#about .links:first-of-type {
  tabellen-ausrichtung: rechts;
}
#about .links:first-of-type bild {
  rahmen: 2px durchgezogen #dfdfdf;
  gleiten: rechts;
  außenrand: 0 60px 0 10px;
}
#about .links:first-of-type liste {
  außenrand-oben: 30px;
  farbe: #717171;
}
#about .links:first-of-type liste listenelement {
  außenrand: 2px 0;
}
#about .links:first-of-type liste listenelement anker {
  farbe: #717171;
}
#about .links:first-of-type liste listenelement:danach {
  inhalt: " \2022";
}
#about .links:last-of-type bild {
  rahmen: 2px durchgezogen #dfdfdf;
  gleiten: links;
  außenrand: 0 10px 0 60px;
}
#about .links:last-of-type liste {
  außenrand-oben: 30px;
  farbe: #717171;
}
#about .links:last-of-type liste listenelement {
  außenrand: 2px 0;
}
#about .links:last-of-type liste listenelement anker {
  farbe: #717171;
}
#about .links:last-of-type liste listenelement:davor {
  inhalt: "\2022  ";
}
footer {
  hintergrund: url('../bild/footer_bg.png') repeat-x unten links;
  außenrand-oben: 100px;
  höhe: 51px;
  farbe: #717171;
}
footer .links {
  außenrand-oben: 10px;
}
footer liste {
  außenrand-oben: 10px;
}
footer liste listenelement {
  gleiten: links;
}
footer liste listenelement anker {
  farbe: #717171;
}
footer liste listenelement:nicht(:letzes-kind) {
  außenrand: 0 5px;
}