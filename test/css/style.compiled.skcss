.icon {
  zeiger: pointer;
  anzeige: block;
  breite: 32px;
  höhe: 32px;
  hintergrund: #333;
  gleiten: links;
  -webkit-rahmen-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  rahmen-links: 1px durchgezogen #808080;
  rahmen-rechts: 1px durchgezogen #1a1a1a;
  position: relativ;
  überlauf: hidden;
}
.icon.single {
  rahmen: keines;
}
.icon.links,
.icon.links .inner {
  -webkit-rahmen-radius: 0 2px 2px 0;
  -moz-border-radius: 0 2px 2px 0;
  border-radius: 0 2px 2px 0;
}
.icon.rechts,
.icon.right .inner {
  -webkit-rahmen-radius: 2px 0 0 2px;
  -moz-border-radius: 2px 0 0 2px;
  border-radius: 2px 0 0 2px;
}
.icon .inner {
  anzeige: block;
  breite: 200%;
  höhe: 200%;
  position: absolut;
  oben: 0;
  links: 0;
  filter: alpha(opacity=55);
  -khtml-opacity: 0.55;
  -moz-opacity: 0.55;
  opacity: 0.55;
}
.icon:erstes-kind,
.icon:first-child .inner {
  -webkit-rahmen-radius: 2px 0 0 2px;
  -moz-border-radius: 2px 0 0 2px;
  border-radius: 2px 0 0 2px;
  rahmen-links: keines;
}
.icon:letzes-kind,
.icon:last-child .inner {
  -webkit-rahmen-radius: 0 2px 2px 0;
  -moz-border-radius: 0 2px 2px 0;
  border-radius: 0 2px 2px 0;
  rahmen-rechts: keines;
}
.icon .list {
  hintergrund: url("../bild/icons/icon-list.png") links oben;
}
.icon .grid {
  hintergrund: url("../img/icons/icon-grid.png") links top;
}
.icon .close {
  hintergrund: url("../bild/icons/icon-close.png") links oben;
}
.icon .cross {
  hintergrund: url("../img/icons/icon-cross.png") links top;
}
.icon .up {
  hintergrund: url("../bild/icons/arrow-up.png") links oben;
}
.icon .down {
  hintergrund: url("../img/icons/arrow-down.png") links top;
}
.icon .left {
  hintergrund: url("../bild/icons/arrow-links.png") links oben;
}
.icon .rechts {
  hintergrund: url("../img/icons/arrow-right.png") links top;
}
.icon:über .inner,
.icon.aktiv .inner {
  filter: alpha(opacity=100);
  -khtml-opacity: 1;
  -moz-opacity: 1;
  opacity: 1;
  oben: -100%;
}
.icon.aktiv {
  hintergrund-farbe: #666;
  hintergrund-wiederholung: repeat-x;
  hintergrund-bild: -khtml-gradient(linear, links oben, links unten, from(rgba(0,0,0,0.40)), to(#666));
  background-image: -moz-linear-gradient(top, rgba(0,0,0,0.40), #666);
  background-image: -ms-linear-gradient(top, rgba(0,0,0,0.40), #666);
  background-image: -webkit-gradient(linear, links top, left bottom, color-stop(0%, rgba(0,0,0,0.40)), color-stop(100%, #666));
  background-image: -webkit-linear-gradient(top, rgba(0,0,0,0.40), #666);
  background-image: -o-linear-gradient(top, rgba(0,0,0,0.40), #666);
  background-image: linear-gradient(top, rgba(0,0,0,0.40), #666);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='rgba(0,0,0,0.40)', endColorstr='#666', GradientType=0);
  -webkit-box-shadow: inset 0 8px 15px rgba(0,0,0,0.66), inset 0 5px 5px rgba(0,0,0,0.66);
  -moz-box-shadow: inset 0 8px 15px rgba(0,0,0,0.66), inset 0 5px 5px rgba(0,0,0,0.66);
  box-shadow: inset 0 8px 15px rgba(0,0,0,0.66), inset 0 5px 5px rgba(0,0,0,0.66);
}
.icon.horizontal:über .inner,
.icon.horizontal.aktiv .inner {
  oben: 0;
  links: -100%;
}
